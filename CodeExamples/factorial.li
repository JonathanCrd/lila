lila factorial

var int number;

func int factorial(int n){
    var int fact, i;
    fact = 1;
    i=1;
    while(i<=n){
        fact = fact * i;
        i = i + 1;
    }
    return fact;
} 

main
{        
    getinput(number, "Give a number: ");
    display("Factorial: ");
    display(factorial(number));
}